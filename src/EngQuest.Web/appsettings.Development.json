{
  "ConnectionStrings": {
    "Database": "Host=localhost;Port=5432;Database=eng-quest;Username=postgres;Password=postgres;",
    "Cache": "localhost:6379"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "Seq",
        "Args": { "serverUrl": "http://localhost:8081" }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithThreadId" ]
  },
  "Authentication": {
    "Audience": "account",
    "Issuer": "http://localhost:18080/realms/eng-quest",
    "MetadataUrl": "http://localhost:18080/realms/eng-quest/.well-known/openid-configuration",
    "RequireHttpsMetadata": false
  },
  "Keycloak": {
    "BaseUrl": "http://localhost:18080",
    "AdminUrl": "http://localhost:18080/admin/realms/eng-quest/",
    "TokenUrl": "http://localhost:18080/realms/eng-quest/protocol/openid-connect/token",
    "AdminClientId": "eng-quest-admin-client",
    "AdminClientSecret": "UZDmbNxWmV4TlpaCRcju6pMRsyuV3er1",
    "AuthClientId": "eng-quest-auth-client",
    "AuthClientSecret": "3E3yvXaYppoYBF3Ir6DgtEzADKKzSurZ"
  },
  "Outbox": {
    "IntervalInSeconds": 10,
    "BatchSize": 10
  }
}
